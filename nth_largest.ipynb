   "source": [
    "def Find_Total(Rank, Array):\n",
    "    if Rank == 1:\n",
    "        return max(Array)\n",
    "    Top_Rank = max(Array)\n",
    "    New_Array = list(filter(lambda a: a != Top_Rank, Array))\n",
    "    return(Find_Total(Rank-1, New_Array))"
   ]
  },
